{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "clipboard:read-all",
    "clipboard:write-all",
    "clipboard:monitor-all",
    "sql:default",
    "sql:allow-execute",
    "core:window:allow-minimize",
    "core:window:allow-hide",
    "core:window:allow-start-dragging",
    "os:default",
    "autostart:default"
  ]
}